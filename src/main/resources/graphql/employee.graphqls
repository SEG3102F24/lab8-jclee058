type Query {
    employees: [Employee]
    employeeById(employeeId: ID!): Employee
    employeeByName(employeeName: String!): Employee
}

type Employee {
    employeeId: ID!
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String
    email: String
}

input CreateEmployeeInput {
    name: String,
    dateOfBirth: String,
    city: String,
    salary: Float,
    gender: String,
    email: String
}

type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!) : Employee!
    deleteEmployee(employeeId: ID!) : Boolean
}
